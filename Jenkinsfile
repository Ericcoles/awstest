pipeline {
  agent any

  stages {

    stage ("build"){
      steps{
        // sh `npm i`
        echo "building my app..."
      }
    }

    stage ("test") {
      steps{
        echo "testing my app.."
      }
    }
    stage ("deploy") {
      steps {
        echo "deploying my app..."
      }
    }

  }
}